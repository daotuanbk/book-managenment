{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=development node --inspect --trace-warnings .next/production-server/index.js -p $PORT",
    "build": "next build && tsc --project tsconfig.server.json",
    "postbuild": "shx cp -r static .next/",
    "prebuild": "shx rm -rf .next/ ",
    "start": "cross-env NODE_ENV=production node ~/.next/production-server/index.js -p $PORT",
    "initdb": "ts-node --project tsconfig.server.json initdb.ts"
  },
  "dependencies": {
    "@rematch/core": "1.0.0-beta.4",
    "@zeit/next-css": "^0.2.0",
    "@zeit/next-source-maps": "^0.0.2",
    "@zeit/next-typescript": "1.1.0",
    "antd": "^3.8.2",
    "babel-plugin-import": "^1.8.0",
    "bcryptjs": "^2.4.3",
    "braft-editor": "^1.9.7",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.6.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.5",
    "moment": "^2.22.2",
    "mongoose": "^5.2.9",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "next": "^6.1.1",
    "nodemailer": "^4.6.8",
    "qs": "^6.5.2",
    "react": "^16.4.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-email-editor": "^0.8.2",
    "react-i18next": "^7.11.1",
    "react-redux": "^5.0.7",
    "react-scroll": "^1.7.10",
    "shx": "^0.3.2",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/joi": "^13.4.3",
    "@types/js-cookie": "^2.1.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.116",
    "@types/mongoose": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/next": "latest",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.0.36",
    "@types/react-redux": "^6.0.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "ts-node": "^7.0.1"
  },
  "license": "ISC"
}
